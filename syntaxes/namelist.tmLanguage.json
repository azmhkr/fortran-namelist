{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "namelist",
	"patterns": [
		{
			"include": "#strings"
		},
		{
			"include": "#strings_single"
		},
		{
			"include": "#comment"
		}
	],
	"repository": {
		"comment": {
			"patterns": [
				{
					"name": "comment.line.namelist",
					"match": "!.*"
				}
			]
		},
		"strings": {
			"name": "string.quoted.double.namelist",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.namelist",
					"match": "\\\\."
				}
			]
		},
		"strings_single": {
			"name": "string.quoted.namelist",
			"begin": "'",
			"end": "'",
			"patterns": [
				{
					"name": "constant.character.escape.namelist",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "source.nml"
}
